{
   "name": "personal-site",
   "version": "0.0.1",
   "main": "build/index.js",
   "description": "My personal site",
   "dependencies": {
      "body-parser": "^1.19.0",
      "chalk": "^2.4.2",
      "dotenv": "^8.0.0",
      "ejs": "^2.6.1",
      "express": "^4.17.1",
      "node-fetch": "^2.6.0",
      "reflect-metadata": "^0.1.10",
      "rollup-plugin-babel": "^4.3.2",
      "sqlite3": "^4.0.8",
      "typeorm": "^0.2.18"
   },
   "scripts": {
      "build": "run-p copy:templates grunt:build tsc:build",
      "copy:templates": "cpx src/**/*.ejs build/ --include-empty-dirs",
      "grunt:build": "grunt",
      "grunt:watch": "grunt watch",
      "start": "ts-node src/index.ts",
      "tsc:build": "tsc -b",
      "tsnd:watch": "tsnd --pretty --project ./tsconfig.json --respawn --transpile-only --inspect -- src/index.ts",
      "watch": "run-p grunt:watch tsnd:watch"
   },
   "devDependencies": {
      "@babel/core": "^7.4.5",
      "@babel/preset-env": "^7.4.5",
      "@types/express": "^4.17.0",
      "@types/node": "^8.0.29",
      "@types/node-fetch": "^2.3.5",
      "@typescript-eslint/eslint-plugin": "^1.9.0",
      "@typescript-eslint/parser": "^1.9.0",
      "autoprefixer": "^9.6.0",
      "cpx": "^1.5.0",
      "cssnano": "^4.1.10",
      "eslint": "^5.16.0",
      "eslint-config-airbnb-base": "^13.1.0",
      "eslint-plugin-import": "^2.17.3",
      "grunt": "^1.0.4",
      "grunt-contrib-watch": "^1.1.0",
      "grunt-postcss": "^0.9.0",
      "grunt-rollup": "^10.0.0",
      "grunt-sass": "^3.0.2",
      "npm-run-all": "^4.1.5",
      "rollup": "^1.15.5",
      "rollup-plugin-node-builtins": "^2.1.2",
      "sass": "^1.21.0",
      "ts-node": "3.3.0",
      "ts-node-dev": "^1.0.0-pre.39",
      "typescript": "^3.5.1"
   }
}
